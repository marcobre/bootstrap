# moves focus between spaces 
cmd + alt - 1 : yabai -m space --focus 1
cmd + alt - 2 : yabai -m space --focus 2
cmd + alt - 3 : yabai -m space --focus 3
cmd + alt - 4 : yabai -m space --focus 4
cmd + alt - 5 : yabai -m space --focus 5

# move window and focus desktop
shift + alt - 1 : yabai -m window --space 1; yabai -m space --focus 1
shift + alt - 2 : yabai -m window --space 2; yabai -m space --focus 2
shift + alt - 3 : yabai -m window --space 3; yabai -m space --focus 3
shift + alt - 4 : yabai -m window --space 4; yabai -m space --focus 4
shift + alt - 5 : yabai -m window --space 5; yabai -m space --focus 5

# focus window
cmd + alt - h : yabai -m window --focus west
cmd + alt - j : yabai -m window --focus south
cmd + alt - k : yabai -m window --focus north
cmd + alt - l : yabai -m window --focus east

# rotate tree
cmd + alt - r : yabai -m space --rotate 90

# balance window sizes
cmd + alt - c : yabai -m space --balance

# toggle window native fullscreen
cmd + alt - f : yabai -m window --toggle native-fullscreen

# toggle window fullscreen zoom
cmd + alt - z : yabai -m window --toggle zoom-fullscreen

# toggle window float
cmd + alt - x : yabai -m window --toggle float
